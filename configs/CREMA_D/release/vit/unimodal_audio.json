{
  "training_params": {
    "seed": 19,
    "batch_size": 8
  },
  "scheduler": {
    "type":"cosanneal",
    "max_lr": 0.001,
    "warm_up_steps": 1500},
  "optimizer": {
    "type": "Adam",
    "learning_rate": 0.00005,
    "momentum": 0.9,
    "weight_decay":  5e-6,
    "beta1": 0.95,
    "beta2": 0.999
  },
  "early_stopping": {
    "max_epoch": 1500,
    "end_of_epoch_check": true,
    "log_interval": 10,
    "save_every_valstep": 1,
    "validate_every": 125,
    "validate_after": 0,
    "n_steps_stop": 300,
    "n_steps_stop_after": 0,
    "validate_with": "accuracy"
  },
  "dataset": {
      "return_data" : {"video": false, "spectrogram":false, "audio":true, "face": false},
      "sampling_rate": 16000,
      "data_split": {
        "fold": 1
      }
  },
  "model": {
    "model_class": "Audio_Wav2Vec",
    "args": {
      "d_model": 512, "num_classes": 6, "fc_inner": 64,"dropout": 0.1,
      "bias_infusion": {
        "method": false
      },
      "multi_loss": {
        "multi_supervised_w": {
             "combined": 1
          }
      }
    },
    "load_ongoing": false,
    "save_dir": "unimodal_audio_{}.pth.tar"
  }
}