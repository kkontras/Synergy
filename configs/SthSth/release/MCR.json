{
  "model": {
    "model_class": "All2Model_ShuffleGrad",
    "args": {
      "activate_model": {"flow": true, "video": true},
      "swin_backbone": "swin_pretraining_kinetikcs400_1k.pth",
      "pretraining_paths": {
        "flow": "flow_test_kinetics_encoder_fold{}.pth.tar",
        "video": "video_test_encoder_fold{}.pth.tar"
      },
      "clip_grad": true,
      "clip_value": 5.0,
      "d_model": 512,
      "num_frames":16,
      "num_classes": 174,
      "bias_infusion": {
        "method": "ShuffleGradFinal",
        "regby": "greedy",
        "num_samples": 16,
        "l": 0.1,
        "l_gradnorm": 0,
        "shuffle": true,
        "noise": false,
        "starting_epoch": 0,
        "ending_epoch": 1500,
        "use": true,
        "plot": false
      },
      "multi_loss": {
        "multi_supervised_w": {
             "combined": 1, "c": 1, "g": 1
          }
      }
    },
    "load_ongoing": true,
    "save_dir": "MCR_{}.pth.tar"
  }
}