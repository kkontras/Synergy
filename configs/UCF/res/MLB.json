{
  "model": {
    "model_class": "Base_Model",
    "args": {
      "d_model": 512, "num_classes": 51, "fc_inner": 64, "dropout": 0.1,
      "shared_pred": true,
      "cls_type": "linear",
      "clip_grad": false,
      "bias_infusion": {
        "method": "OGM-Mine",
        "alpha": 1.0,
        "starting_epoch": 0,
        "ending_epoch": 1500,
        "use": true,
        "plot": false
      },
      "multi_loss": {
        "multi_supervised_w": {
             "combined": 1, "c": 1, "g": 1
          }
      }
    },
    "load_ongoing": false,
    "save_dir": "MLB_{}.pth.tar",
    "encoders": [
      {
        "model": "Audio_ResNet",
        "args": { "d_model": 512, "num_classes": 51, "fc_inner": 64, "dropout": 0.1, "freeze_encoder": false},
        "pretrainedEncoder": {"use": false, "dir": ""}
      },
      {
        "model": "Video_ResNet",
        "args": {"d_model": 512, "num_classes": 51, "fc_inner": 64, "dropout": 0.1, "freeze_encoder": false},
        "pretrainedEncoder": {"use": false, "dir": ""}}
    ]
  }
}