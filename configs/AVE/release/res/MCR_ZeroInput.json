{
  "model": {
    "model_class": "MCR_ZeroInput_Model",
    "args": {
      "d_model": 512, "num_classes": 28, "fc_inner": 64, "dropout": 0.1,
      "shared_pred": true,
      "cls_type": "linear",
      "clip_grad": false,
      "shufflegradmulti": true,
      "bias_infusion": {
        "method": "ShuffleGradFinal",
        "regby": "dist_pred_cub",
        "num_samples": 6,
        "l": 0.1,
        "l_gradnorm": 0,
        "shuffle": true,
        "noise": false,
        "starting_epoch": 0,
        "ending_epoch": 1500,
        "use": true,
        "plot": false
      },
      "multi_loss": {
        "multi_supervised_w": {
             "combined": 1, "c": 1, "g": 1
          }
      }
    },
    "load_ongoing": false,
    "save_dir": "MCR_ZeroInput_{}.pth.tar",
    "encoders": [
      {
        "model": "Audio_ResNet",
        "args": { "d_model": 512, "num_classes": 28, "fc_inner": 64, "dropout": 0.1, "freeze_encoder": false},
        "pretrainedEncoder": {"use": true, "dir": "audio_linearcls_fold{}.pth.tar"}
      },
      {
        "model": "Video_ResNet",
        "args": {"d_model": 512, "num_classes": 28, "fc_inner": 64, "dropout": 0.1, "freeze_encoder": false},
        "pretrainedEncoder": {"use": true, "dir": "images_linearcls_fold{}.pth.tar"}}
    ]
  }
}