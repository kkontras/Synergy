{
  "model": {
    "model_class": "MCR_ZeroLatent_Model",
    "args": {
      "d_model": 40,
      "num_classes": 1,
      "fc_inner": 64,
      "dropout": 0.1,
      "cls_type": "linear",
      "clip_grad": false,
      "bias_infusion": {
        "method": "ShuffleGradFinal_Reg",
        "regby": "dist_pred_cub",
        "num_samples": 1024,
        "l": 0.1,
        "l_gradnorm": 0,
        "shuffle": true,
        "shuffle_type": "rand",
        "noise": false,
        "starting_epoch": 0,
        "ending_epoch": 1500,
        "use": true,
        "plot": false
      },
    "multi_loss": {
      "multi_supervised_w": {
        "combined": 1, "c": 1, "g": 1
      }
    }
    },
    "load_ongoing": false,
    "save_dir": "MCR_ZeroLatent_{}.pth.tar",
    "encoders": [
      {
        "model": "FactorCL_Uni",
        "args": {
          "n_features": 35,
          "hidden_size": 40,
          "modality": "c",
          "num_classes": 1,
          "freeze_encoder": false},
        "pretrainedEncoder": {
          "use": true,
          "dir": "unimodal_video_fold{}.pth.tar"
        }
      },
      {
        "model": "FactorCL_Uni",
        "args": {
          "n_features": 300,
          "hidden_size": 40,
          "modality": "g",
          "num_classes": 1,
          "freeze_encoder": false},
        "pretrainedEncoder": {
          "use": true,
          "dir": "unimodal_text_fold{}.pth.tar"
        }
      }
    ]
  }
}