  "lr_finder": false,
  "lr_finder_steps": 100,
  "early_stopping": {
    "max_epoch": 1000,
    "end_of_epoch_check": true,
    "log_interval": 10,
    "save_every": 1,
    "validate_every": 127,
    "validate_after": 0,
    "n_steps_stop": 100,
    "n_steps_stop_after": 0,
    "validate_with": "accuracy"
  },
  "dataset": {
      "dataloader_class": "UCF101_Dataloader",
      "data_roots": "/esat/smcdata/users/kkontras/Image_Dataset/no_backup/UCF101",
      "fps": 1,
      "num_frame": 4,
      "fold": 1
  },
  "model": {
    "model_class": "Audio_ResNet",
    "args": {
      "num_classes": 51, "d_model": 512,  "fc_inner": 64, "dropout": 0.1,
      "shared_pred": false,
      "multi_loss": {
        "multi_supervised_w": {
             "combined": 1
          }
      }
    },
    "load_ongoing": true,
    "save_dir": "/esat/smcdata/users/kkontras/Image_Dataset/no_backup/data/Balance_Final/UCF/res/unimodal_audio_{}.pth.tar"
  }
}