{
  "model": {
    "model_class": "ConcatClassifier_CREMAD_OGM_ShuffleGradEP_pre",
    "args": {
      "d_model": 512, "num_classes": 28, "fc_inner": 64, "dropout": 0.1,
      "shared_pred": true,
      "cls_type": "linear",
      "clip_grad": false,
      "bias_infusion": {
        "method": "MLB_Shap",
        "balance_mode": "perm",
        "shuffle_type": "rand",
        "shuffle": true,
        "num_samples": 16,
        "alpha": 0.8,
        "starting_epoch": 0,
        "ending_epoch": 1500,
        "use": true,
        "plot": false
      },
      "multi_loss": {
        "multi_supervised_w": {
             "combined": 1, "c": 1, "g": 1
          }
      }
    },
    "load_ongoing": false,
    "save_dir": "MLB_PermEq11_{}.pth.tar",
    "encoders": [
      {
        "model": "Audio_ResNet",
        "args": { "d_model": 512, "num_classes": 28, "fc_inner": 64, "dropout": 0.1, "freeze_encoder": false},
        "pretrainedEncoder": {"use": false, "dir": ""}
      },
      {
        "model": "Video_ResNet",
        "args": {"d_model": 512, "num_classes": 28, "fc_inner": 64, "dropout": 0.1, "freeze_encoder": false},
        "pretrainedEncoder": {"use": false, "dir": ""}}
    ]
  }
}