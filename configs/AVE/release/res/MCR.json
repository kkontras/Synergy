{
  "model": {
    "model_class": "ConcatClassifier_CREMAD_OGM_ShuffleGradEP_pre",
    "args": {
      "d_model": 512, "num_classes": 28, "fc_inner": 64, "dropout": 0.1,
      "shared_pred": true,
      "cls_type": "linear",
      "clip_grad": false,
      "shufflegradmulti": true,
      "bias_infusion": {
        "method": "MCR",
        "regby": "greedy",
        "num_samples": 6,
        "l": 0.1,
        "shuffle": true,
        "starting_epoch": 0,
        "ending_epoch": 1500,
        "use": true
      },
      "multi_loss": {
        "multi_supervised_w": {
             "combined": 1, "c": 1, "g": 1
          }
      }
    },
    "load_ongoing": false,
    "save_dir": "MCR_{}.pth.tar",
    "encoders": [
      {
        "model": "Audio_ResNet",
        "args": { "d_model": 512, "num_classes": 28, "fc_inner": 64, "dropout": 0.1, "freeze_encoder": false},
        "pretrainedEncoder": {"use": true, "dir": "unimodal_audio_fold{}.pth.tar"}
      },
      {
        "model": "Video_ResNet",
        "args": {"d_model": 512, "num_classes": 28, "fc_inner": 64, "dropout": 0.1, "freeze_encoder": false},
        "pretrainedEncoder": {"use": true, "dir": "unimodal_video_fold{}.pth.tar"}}
    ]
  }
}