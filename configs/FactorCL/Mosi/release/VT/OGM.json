{
  "model": {
    "args": {
      "bias_infusion": {
        "method": "OGM_Reg",
        "alpha": 1.0,
        "starting_epoch": 0,
        "ending_epoch": 1500,
        "use": true,
        "plot": false
      }
    },
    "save_dir": "OGM_{}.pth.tar",
    "encoders": [
      {
        "model": "FactorCL_Uni",
        "args": {
          "n_features": 20,
          "hidden_size": 40,
          "modality": "c",
          "num_classes": 1,
          "freeze_encoder": false
        },
        "pretrainedEncoder": {
          "use": false,
          "dir": "unimodal_video_{}.pth.tar"
        }
      },
      {
        "model": "FactorCL_Uni",
        "args": {
          "n_features": 300,
          "hidden_size": 40,
          "modality": "g",
          "num_classes": 1,
          "freeze_encoder": false
        },
        "pretrainedEncoder": {
          "use": false,
          "dir": "unimodal_text_{}.pth.tar"
        }
      }
    ]
  }
}