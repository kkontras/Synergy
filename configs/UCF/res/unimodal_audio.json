{
  "exp_name": "UCF101 Experiment",
  "agent": "Agent",
  "mode": "train",
  "num_classes": 5,
  "training_params": {
    "cuda": true,
    "gpu_device": [0],
    "seed": 109,
    "batch_size": 32,
    "test_batch_size": 32,
    "data_loader_workers": 8,
    "pin_memory": true,
    "async_loading": true,
    "validation": true,
    "rec_test": false,
    "test_on_bottoms": false,
    "use_test_set": true,
    "verbose": true,
    "adversarial_training": {
      "use": false,
      "adv_epsilon": 0.01
    },
    "tdqm_disable": true,
    "wandb_disable": false,
    "res": true
