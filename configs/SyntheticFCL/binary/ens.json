        }
      },
      {
        "model": "SyntheticGaussianNet_Enc",
        "args": {
          "input_size": 100,
          "hidden_size": 100,
          "modality": 1,
          "num_classes": 2,
          "freeze_encoder": false
        },
        "pretrainedEncoder": {
          "use": true,
