{
  "model": {
    "model_class": "Base_3D_Model",
    "args": {
      "d_model": 40,
      "num_classes": 1,
      "fc_inner": 64,
      "dropout": 0.1,
      "cls_type": "linear",
      "clip_grad": false,
      "bias_infusion": {
        "method": false
      },
    "multi_loss": {
      "multi_supervised_w": {
        "combined": 1,
        "c": 0,
        "g": 0
      }
    }
    },
    "load_ongoing": false,
    "save_dir": "pre_frozen_{}.pth.tar",
    "encoders": [
      {
        "model": "FactorCL_Uni",
        "args": {
          "n_features": 35,
          "hidden_size": 40,
          "modality": "c",
          "num_classes": 1,
          "freeze_encoder": true},
        "pretrainedEncoder": {
          "use": true,
          "dir": "unimodal_video_{}.pth.tar"
        }
      },
      {
        "model": "FactorCL_Uni",
        "args": {
          "n_features": 300,
          "hidden_size": 40,
          "modality": "g",
          "num_classes": 1,
          "freeze_encoder": true},
        "pretrainedEncoder": {
          "use": true,
          "dir": "unimodal_text_{}.pth.tar"
        }
      },
      {
        "model": "FactorCL_Uni",
        "args": {
          "n_features": 74,
          "hidden_size": 40,
          "modality": "f",
          "num_classes": 1,
          "freeze_encoder": true},
        "pretrainedEncoder": {
          "use": true,
          "dir": "unimodal_audio_{}.pth.tar"
        }
      }
    ]
  }
}