{
  "training_params": {
    "batch_size": 32,
    "test_batch_size": 32,
    "tdqm_disable": false
  },
  "early_stopping": {
    "end_of_epoch_check": true,
    "n_steps_stop": 50,
    "save_every_valstep": 10,
    "validate_every": 25,
    "validate_with": "loss"
  },
  "optimizer": {
    "type": "Adam",
    "learning_rate": 0.00001,
    "momentum": 0.9,
    "weight_decay": 0.00001,
    "beta1": 0.9,
    "beta2": 0.999
  },
  "dataset": {
      "norm": false,
      "return_data" : {"video": true, "spectrogram":true, "audio":false, "face": false},
      "sampling_rate": 22050,
      "data_split": {
        "fold": 0
      }
  },
  "model": {
    "model_class": "Model_Generator",
    "args": {
      "d_model": 512, "num_classes": 6, "fc_inner": 64, "dropout": 0.1,
      "cls_type": "linear",
      "clip_grad": false,
      "bias_infusion": {
        "method": "false",
        "l": 0.1,
        "starting_epoch": 0,
        "ending_epoch": 1500,
        "use": true,
        "plot": false
      },
      "multi_loss": {
        "multi_supervised_w": {
             "combined": 1, "c": 0, "g": 0
          }
      }
    },
    "load_ongoing": false,
    "save_dir": "Synprom_IB_hingegans{}.pth.tar",
    "encoders": [
      {
        "model": "Audio_ResNet",
        "args": { "d_model": 512, "num_classes": 6, "fc_inner": 64, "dropout": 0.1, "freeze_encoder": true},
        "pretrainedEncoder": {"use": true, "dir": "unimodal_audio_fold{}_lr0.001_wd0.0001.pth.tar"}
      },
      {
        "model": "Video_ResNet",
        "args": {"d_model": 512, "num_classes": 6, "fc_inner": 64, "dropout": 0.1, "freeze_encoder": true},
        "pretrainedEncoder": {"use": true, "dir": "unimodal_video_fold{}_lr0.001_wd0.0001.pth.tar"}
      },
      {
        "model": "Z1Generator_GAN",
        "args": { "dim_in": 512, "dim_out": 512, "hidden_dim": 256, "bottleneck_dim": 128, "noise_dim": 32, "lambda_rec": 0.5, "lambda_d": 0.5, "freeze_encoder": false},
        "pretrainedEncoder": {"use": false, "dir": "z1toz2_gan_fold{}.pth.tar"},
        "save_encoder": true
      },
      {
        "model": "Z1Generator_GAN",
        "args": { "dim_in": 512, "dim_out": 512,"hidden_dim": 256, "bottleneck_dim": 128, "noise_dim": 32,  "lambda_rec": 0.5,  "lambda_d": 0.5, "freeze_encoder": false},
        "pretrainedEncoder": {"use": false, "dir": "z2toz1_gan_fold{}.pth.tar"},
        "save_encoder": true
      }
    ]
  }
}