  "scheduler": {"type":"cosanneal", "max_lr": 0.03, "warm_up_steps": 780},
  "optimizer": {
    "type": "Adam",
    "learning_rate": 0.001,
    "momentum": 0.9,
    "weight_decay": 0.0001,
    "beta1": 0.9,
    "beta2": 0.999
