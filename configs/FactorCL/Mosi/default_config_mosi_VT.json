{
  "exp_name": "Mosi Experiment",
  "agent": "Agent",
  "task": "regression",
  "training_params": {
    "cuda": true,
    "gpu_device": [0],
    "seed": 109,
    "batch_size": 32,
    "test_batch_size": 32,
    "data_loader_workers": 8,
    "pin_memory": true,
    "async_loading": true,
    "validation": true,
    "rec_test": true,
    "test_on_bottoms": false,
    "use_test_set": true,
    "verbose": true,
    "adversarial_training": {
      "use": false,
      "adv_epsilon": 0.01
    },
    "tdqm_disable": false,
    "wandb_disable": true,
    "res": true
  },
  "scheduler": {"type":"cosanneal", "max_lr": 0.03, "warm_up_steps": 780},
  "optimizer": {
    "type": "Adam",
    "learning_rate": 0.0001,
    "momentum": 0.9,
    "weight_decay": 0.0001,
    "beta1": 0.9,
    "beta2": 0.999
  },
  "early_stopping": {
    "max_epoch": 100,
    "end_of_epoch_check": true,
    "log_interval": 10,
    "save_every_step": 3000,
    "validate_every": 185,
    "save_every_valstep": 50,
    "validate_after": 0,
    "n_steps_stop": 30,
    "n_steps_stop_after": 0
  },
  "dataset": {
      "dataloader_class": "FactorCL_Dataloader",
      "data_roots": "/esat/smcdata/users/kkontras/Image_Dataset/no_backup/FactorCL/mosi_data.pkl"
  },
  "model": {
    "model_class": "Base_Model",
    "save_base_dir": "/esat/smcdata/users/kkontras/Image_Dataset/no_backup/data/Balance_Final/MOSI/VT",
    "args": {
      "d_model": 40,
      "num_classes": 1,
      "fc_inner": 64,
      "dropout": 0.1,
      "cls_type": "linear",
      "clip_grad": false,
      "bias_infusion": {
        "method": false
      },
    "multi_loss": {
      "multi_supervised_w": {
        "combined": 1,
        "c": 0,
        "g": 0
      }
    }
    },
    "load_ongoing": false,
    "save_dir": "{}.pth.tar",
    "encoders": [
      {
        "model": "FactorCL_Uni",
        "args": {
          "n_features": 20,
          "hidden_size": 40,
          "modality": "c",
          "num_classes": 1,
          "freeze_encoder": false},
        "pretrainedEncoder": {
          "use": true,
          "dir": "unimodal_video_{}.pth.tar"
        }
      },
      {
        "model": "FactorCL_Uni",
        "args": {
          "n_features": 300,
          "hidden_size": 40,
          "modality": "g",
          "num_classes": 1,
          "freeze_encoder": false},
        "pretrainedEncoder": {
          "use": true,
          "dir": "unimodal_text_{}.pth.tar"
        }
      }
    ]
  }
}
