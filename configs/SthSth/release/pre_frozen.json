{
  "model": {
    "model_class": "All2Model_OGM",
    "args": {
      "activate_model": {"flow": true, "video": true},
      "swin_backbone": "swin_pretraining_kinetikcs400_1k.pth",
      "frozen_encoders": true,
      "pretraining_paths": {
        "flow": "flow_test_kinetics_encoder_fold{}.pth.tar",
        "video": "video_test_encoder_fold{}.pth.tar"
      },
      "clip_grad": true,
      "clip_value": 5.0,
      "d_model": 512,
      "num_frames":16,
      "num_classes": 174,
      "bias_infusion": {
        "method": false
      },
      "multi_loss": {
        "multi_supervised_w": {
             "combined": 1
          }
      }
    },
    "load_ongoing": true,
    "save_dir": "pre_frozen_{}.pth.tar"
  }
}