{
  "model": {
    "model_class": "All2Model_OGM",
    "args": {
      "activate_model": {"flow": true, "video": true},
      "swin_backbone": "swin_pretraining_kinetikcs400_1k.pth",
      "clip_grad": true,
      "clip_value": 5.0,
      "d_model": 512,
      "num_frames":16,
      "num_classes": 174,
      "bias_infusion": {
        "method": "ReconBoost",
        "alpha": 0.8,
        "ensemble_stages": 4,
        "epoch_stages": 4,
        "use_ga": true,
        "weight1": 5.0,
        "weight2": 1.0,
        "starting_epoch": 0,
        "ending_epoch": 1500,
        "use": true,
        "plot": false
      },
      "multi_loss": {
        "multi_supervised_w": {
            "combined": 1, "c": 1, "g": 1
          }
      }
    },
    "load_ongoing": true,
    "save_dir": "Reconboost_{}.pth.tar"
  }
}